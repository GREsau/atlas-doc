{
  "name": "atlas-doc",
  "version": "0.1.4",
  "description": "Easily create Atlassian Documents for use with the Stride API",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "standard --verbose src/**/*.js test/**/*.js | snazzy",
    "test": "tape -r ./test/setupBabel.js test/**/*.js | tap-summary",
    "coverage": "cross-env BABEL_ENV=coverage nyc --reporter=html --reporter=json --reporter=text npm test",
    "clean": "rimraf dist/*",
    "compile": "babel src --out-dir dist",
    "build": "npm run lint && npm test && npm run clean && npm run compile",
    "prepublishOnly": "npm run build"
  },
  "author": "Graham Esau",
  "license": "ISC",
  "homepage": "https://github.com/GREsau/atlas-doc",
  "repository": {
    "type": "git",
    "url": "https://github.com/GREsau/atlas-doc.git"
  },
  "devDependencies": {
    "adf-builder": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "tap-summary": "^4.0.0",
    "tape": "^4.8.0",
    "tape-catch": "^1.0.6"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
