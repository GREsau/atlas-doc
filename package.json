    "dist"
  ],
  "engines":  { "node" : ">=4.0.0" },
  "scripts": {
    "lint": "standard --verbose src/**/*.js | snazzy",
    "test": "tape -r babel-register test/**/*.js | tap-spec",
    "clean": "rimraf dist/*",
    "compile": "babel src --out-dir dist --ignore **/*.test.js,**/test/**",
    "build": "npm run lint && npm test && npm run clean && npm run compile"
  },
